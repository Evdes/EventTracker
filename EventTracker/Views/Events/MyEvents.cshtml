@model IEnumerable<Event>
@{
    ViewBag.Title = "My events";
}

<div class="col">
    @foreach (var e in Model)
    {
        <div class="card mt-3 pt-4 pb-4 col-lg-4 col-md-9 col-sm-10">

                <partial name="Partials/_EventDetails" model="e" />
                <br />
                <partial name="Partials/_Participants" model="e" />

        </div>
    }
</div>